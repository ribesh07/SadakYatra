        </li>
      </ul>
      <div class="tab-content" id="myTabContent">
        <div class="tab-pane fade" id="home" role="tabpanel" aria-labelledby="home-tab">...</div>
        <div class="tab-pane fade show active" id="profile" role="tabpanel" aria-labelledby="profile-tab"></div>
        <div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">...</div>
      </div>

      <br>
<body>
  <h1>Agent Details Table</h1>
  <br>
  <table id="user-table">
    <thead>
      <tr>
        <th>UserId</th>
        <th>Username</th>
        <th>Email</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>AgentId</td>
        <td>John Doe</td>
        <td>johndoe@example.com</td>
        <td>
          <button class="edit-btn" data-user-id="1" data-first-name="John" data-last-name="Doe" data-email="johndoe@example.com">Edit</button>
          <button class="delete-btn">Delete</button>
        </td>
    </tr>
    <tr>
        <td>AgentId</td>
        <td>Jane Doe</td>
        <td>janedoe@example.com</td>
        <td>
          <button class="edit-btn" data-user-id="2" data-first-name="Jane" data-last-name="Doe" data-email="janedoe@example.com">Edit</button>
          <button class="delete-btn">Delete</button>
        </td>
      </tr>
      <!-- Add more table rows as needed -->
    </tbody>
  </table>
  
  <form id="editForm" style="display: none;" action="/agent-update" method="post">
    <input type="hidden" id="userId" name="userId">
    <input type="text" id="firstName" name="firstName" placeholder="First Name">
    <input type="text" id="lastName" name="lastName" placeholder="Last Name">
    <input type="email" id="email" name="email" placeholder="Email">
    <button type="submit">Save</button>
  </form>

  <script src="users-details.js"></script>
</body>
</html>